generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model url {
  id         BigInt    @id @default(autoincrement())
  url        String?   @db.VarChar
  short_url  String?   @unique @db.VarChar
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
